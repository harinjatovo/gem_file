<p id="notice"><%= notice %></p>

<h1>Gemes</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Description</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gemes.each do |geme| %>
      <tr>
        <td><%= geme.nom %></td>
        <td><%= geme.description %></td>
        <td><%= geme.url %></td>
        <td><%= link_to 'Show', geme %></td>
        <td><%= link_to 'Edit', edit_geme_path(geme) %></td>
        <td><%= link_to 'Destroy', geme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Geme', new_geme_path %>
